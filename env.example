# EmailJS Configuration
# Получите ключи на https://www.emailjs.com/
VITE_EMAILJS_PUBLIC_KEY=your_public_key_here
VITE_EMAILJS_SERVICE_ID=your_service_id_here
VITE_EMAILJS_TEMPLATE_ID=your_template_id_here

# Email Configuration
VITE_RECIPIENT_EMAIL=your_email@example.com
VITE_COMPANY_NAME=Your Company Name

# App Configuration
VITE_APP_TITLE=Your App Title
VITE_APP_DESCRIPTION=Your app description

# Google reCAPTCHA Configuration
# Получите ключи на https://www.google.com/recaptcha/admin
VITE_RECAPTCHA_SITE_KEY=your_recaptcha_site_key_here
# ВАЖНО: VITE_RECAPTCHA_SECRET_KEY не нужен на фронтенде!

# Инструкция:
# 1. Скопируйте этот файл как .env.local
# 2. Замените 'your_*' на реальные значения
# 3. НИКОГДА не коммитьте .env.local в Git!
